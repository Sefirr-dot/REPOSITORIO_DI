package matrixx;

public class MatricesWapen {

	public static void main(String[] args) {
		
		int hola[][] = {{21,222,1,1,2},
						{23,232,3,3,3}};

		for (int i = 0; i < hola.length; i++) {
			for (int j = 0; j < hola[0].length; j++) {
				System.out.print(hola[i][j]);
				
			}
			System.out.println();
		}
	}

}
